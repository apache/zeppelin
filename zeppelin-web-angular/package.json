{
  "name": "zeppelin",
  "version": "0.0.0",
  "scripts": {
    "prepare": "cd .. && husky",
    "postinstall": "npm run build:projects && npm run build:tslint-rules",
    "ng": "./node_modules/.bin/ng",
    "start": "cross-env NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch' ng serve --proxy-config proxy.conf.js --extra-webpack-config webpack.partial.js",
    "build": "cross-env NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch' ng build --configuration production --extra-webpack-config webpack.partial.js",
    "build:tslint-rules": "tsc -p tslint-rules/tsconfig.json",
    "build:projects": "npm run build-project:sdk && npm run build-project:vis",
    "build-project:sdk": "cross-env NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch' ng build --project zeppelin-sdk",
    "build-project:vis": "cross-env NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch' ng build --project zeppelin-visualization",
    "lint": "ng lint && tslint -p tslint-rules/tsconfig.json && prettier --check \"**/*.{ts,js,json,css,html}\"",
    "lint:fix": "ng lint --fix && tslint --fix -p tslint-rules/tsconfig.json && prettier --write \"**/*.{ts,js,json,css,html}\"",
    "e2e": "playwright test",
    "e2e:fast": "playwright test --project=chromium",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:ci": "export CI=true && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:4200"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.4",
    "angular": "^1.8.2",
    "ansi_up": "^4.0.4",
    "core-js": "^2.5.4",
    "d3": "3.5.17",
    "date-fns": "^2.30.0",
    "diff-match-patch": "^1.0.4",
    "github-markdown-css": "^3.0.1",
    "highlight.js": "^9.15.8",
    "jquery": "3.7.1",
    "jquery-ui": "1.14.0",
    "lodash": "^4.17.21",
    "mathjax": "2.7.5",
    "monaco-editor": "0.33.0",
    "ng-zorro-antd": "^12.1.1",
    "nvd3": "1.8.6",
    "parse5": "^5.1.1",
    "rxjs": "~6.5.3",
    "systemjs": "^5.0.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.14.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@playwright/test": "1.53.2",
    "@types/angular": "^1.8.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/highlight.js": "^9.12.3",
    "@types/jquery": "3.5.16",
    "@types/lodash": "4.14.144",
    "@types/mathjax": "^0.0.35",
    "@types/node": "~12.19.16",
    "@types/parse5": "^5.0.2",
    "@types/webpack-env": "^1.18.8",
    "codelyzer": "^6.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^8.0.0",
    "https-proxy-agent": "^2.2.1",
    "husky": "9.1.7",
    "lint-staged": "^8.1.6",
    "monaco-editor-webpack-plugin": "7.1.1",
    "ng-packagr": "^12.2.7",
    "ngx-build-plus": "^12.2.0",
    "nz-tslint-rules": "^0.901.0",
    "prettier": "^1.17.0",
    "scandirectory": "8.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "**/*.ts": [
      "tslint --fix",
      "./node_modules/.bin/prettier --write",
      "git add"
    ],
    "**/*.{js,json,css,html}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  }
}
