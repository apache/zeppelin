<%@ val query: String%>
<%@ val consistency: String%>
<%@ val triedHosts: String%>
<%@ val queriedHosts: String%>
<%@ val schemaInAgreement: String%>
<div class="container">
<div class="row text-center">
<h4>No Result</h4>
</div>
<br/>
    <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6 col-offset-md-3 table-responsive table-bordered">
        <table class="table">
            <caption><h5>Last query execution info</h5></caption>
            <thead>
                <tr>
                    <th>Info</th>
                    <th>Value</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Statement</td>
                    <td>${query}</td>
                </tr>
                <tr>
                    <td>Achieved Consistency</td>
                    <td>${consistency}</td>
                </tr>
                <tr>
                    <td>Tried Hosts</td>
                    <td>${triedHosts}</td>
                </tr>
                <tr>
                    <td>Queried Hosts</td>
                    <td>${queriedHosts}</td>
                </tr>
                <tr>
                    <td>Schema In Agreement</td>
                    <td>${schemaInAgreement}</td>
                </tr>
            </tbody>
        </table>
    </div>
    </div>
</div>